# nexus-backend/.env.example
# Copy to .env and fill in real values before running locally.
# NEVER commit .env — it is in .gitignore

# ── AI Providers ─────────────────────────────────────────────────────────────
GEMINI_API_KEY=
GROK_API_KEY=
CEREBRAS_API_KEY=
MISTRAL_API_KEY=
OPENROUTER_API_KEY=
OPENAI_API_KEY=
ANTHROPIC_API_KEY=

# ── Supabase ──────────────────────────────────────────────────────────────────
SUPABASE_URL=https://your-project-id.supabase.co
SUPABASE_KEY=your-anon-key
SUPABASE_SERVICE_KEY=your-service-role-key

# ── Redis ─────────────────────────────────────────────────────────────────────
REDIS_URL=redis://localhost:6379

# ── Vapi (Voice AI) ───────────────────────────────────────────────────────────
VAPI_API_KEY=
VAPI_ASSISTANT_ID=
VAPI_PHONE_NUMBER_ID=
VAPI_PHONE_NUMBER_ID_IN=
VAPI_PHONE_NUMBER_ID_US=
VAPI_PHONE_NUMBER_ID_UK=
VAPI_PHONE_NUMBER_ID_GLOBAL=

# ── Payments ──────────────────────────────────────────────────────────────────
STRIPE_SECRET_KEY=sk_test_...
STRIPE_WEBHOOK_SECRET=whsec_...
RAZORPAY_KEY_ID=rzp_test_...
RAZORPAY_KEY_SECRET=
RAZORPAY_WEBHOOK_SECRET=

# ── Communications ────────────────────────────────────────────────────────────
SLACK_WEBHOOK_URL=https://hooks.slack.com/services/...
WEBHOOK_BASE_URL=https://your-render-backend.onrender.com

# ── Data / Market ─────────────────────────────────────────────────────────────
POLYGON_API_KEY=
FINNHUB_API_KEY=
SERPER_API_KEY=

# ── Trading ───────────────────────────────────────────────────────────────────
ALPACA_API_KEY=
ALPACA_SECRET_KEY=
ALPACA_ENABLED=false

# ── Observability ─────────────────────────────────────────────────────────────
AGENTOPS_API_KEY=

# ── Security ──────────────────────────────────────────────────────────────────
ADMIN_SECRET=change-me-in-production
DISABLE_INTERPRETABILITY=0

# ── n8n ───────────────────────────────────────────────────────────────────────
N8N_URL=https://n8n.yourdomain.com
