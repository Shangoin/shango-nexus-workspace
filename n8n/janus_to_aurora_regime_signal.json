            }
          ]
        },
        "options": {
          "timeout": 15000
        }
      },
      "id": "node-aurora-increase-outreach",
      "name": "Aurora ‚Äî Increase Outreach 1.5x",
      "type": "n8n-nodes-base.httpRequest",
      "typeVersion": 3,
      "position": [700, 160]
    },
    {
      "parameters": {
        "method": "POST",
        "url": "={{ $env.NEXUS_BACKEND_URL }}/api/war-room/run",
        "authentication": "none",
        "sendBody": true,
        "bodyParameters": {
          "parameters": [
            {
              "name": "task",
              "value": "reduce_outreach_intensity"
            },
            {
              "name": "reason",
              "value": "={{ $json.body.regime }} regime detected ‚Äî reducing outreach"
            },
            {
              "name": "intensity_multiplier",
              "value": "0.6"
            },
            {
              "name": "target_tier",
              "value": "ALL"
            }
          ]
        },
        "options": {
          "timeout": 15000
        }
      },
      "id": "node-aurora-reduce-outreach",
      "name": "Aurora ‚Äî Reduce Outreach",
      "type": "n8n-nodes-base.httpRequest",
      "typeVersion": 3,
      "position": [700, 440]
    },
    {
      "parameters": {
        "method": "POST",
        "url": "={{ $env.NEXUS_BACKEND_URL }}/api/nexus/events",
        "authentication": "none",
        "sendBody": true,
        "bodyParameters": {
          "parameters": [
            {
              "name": "type",
              "value": "janus.regime_signal_sent"
            },
            {
              "name": "regime",
              "value": "={{ $('Regime Change Webhook').item.json.body.regime }}"
            },
            {
              "name": "aurora_notified",
              "value": "true"
            },
            {
              "name": "confidence",
              "value": "={{ $('Regime Change Webhook').item.json.body.confidence }}"
            }
          ]
        },
        "options": {}
      },
      "id": "node-publish-event",
      "name": "Publish Nexus Event",
      "type": "n8n-nodes-base.httpRequest",
      "typeVersion": 3,
      "position": [960, 300]
    },
    {
      "parameters": {
        "webhookUri": "={{ $env.SLACK_WEBHOOK_URL }}",
        "text": "={{ 'üìà Janus‚ÜíAurora: ' + $('Regime Change Webhook').item.json.body.regime + ' regime (' + ($('Regime Change Webhook').item.json.body.confidence * 100).toFixed(0) + '% confidence). Aurora outreach intensity ' + ($('Regime Change Webhook').item.json.body.regime === 'bull' ? 'increased 1.5x üöÄ' : 'reduced ‚ö†Ô∏è') }}",
        "otherOptions": {}
      },
      "id": "node-slack-notify",
      "name": "Slack Notify",
      "type": "n8n-nodes-base.slack",
      "typeVersion": 2,
      "position": [1180, 300]
    },
    {
      "parameters": {
        "respondWith": "json",
        "responseBody": "={ \"status\": \"ok\", \"regime\": \"{{ $('Regime Change Webhook').item.json.body.regime }}\", \"action\": \"aurora_notified\" }"
      },
      "id": "node-respond",
      "name": "Respond to Webhook",
      "type": "n8n-nodes-base.respondToWebhook",
      "typeVersion": 1,
      "position": [1400, 300]
    }
