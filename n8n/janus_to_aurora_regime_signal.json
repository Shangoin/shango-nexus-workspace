        "sendBody": true,
        "bodyParameters": {
          "parameters": [
            {
              "name": "task",
              "value": "increase_outreach"
            },
            {
              "name": "reason",
              "value": "Bull market detected ‚Äî premium clients active"
            },
            {
              "name": "intensity_multiplier",
              "value": "1.5"
            },
            {
              "name": "target_tier",
